; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "BasePiece",
"language": "GDScript",
"path": "res://BasePiece.gd"
}, {
"base": "Node",
"class": "Board",
"language": "GDScript",
"path": "res://Board.gd"
}, {
"base": "ColorRect",
"class": "ColorManager",
"language": "GDScript",
"path": "res://Shaders/Recolor.gd"
}, {
"base": "BasePiece",
"class": "Pawn",
"language": "GDScript",
"path": "res://Pawn.gd"
} ]
_global_script_class_icons={
"BasePiece": "",
"Board": "",
"ColorManager": "",
"Pawn": ""
}

[application]

config/name="2022 Rook Queen"
run/main_scene="res://MainScene.tscn"
config/icon="res://icon.png"

[autoload]

GameManager="*res://GameManager.gd"

[display]

window/size/width=256
window/size/height=256
window/size/test_width=512
window/size/test_height=512
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.639216, 0.713726, 1, 1 )
environment/default_environment="res://default_env.tres"
